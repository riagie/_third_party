{
  "name": "third_party-",
  "version": "0.0.1",
  "description": "The Cloud Data Center API's third party.",
  "keywords": [
    "libraries",
    "server",
    "client",
    "automation",
    "api"
  ],
  "type": "commonjs",
  "license": "MIT",
  "author": {
    "name": "Ogie Nurdiana",
    "email": "ogie.nurdiana447@gmail.com",
    "homepage": "https://ogienurdiana.com",
    "role": "developer"
  },
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "schema:sync": "npx typeorm-ts-node-commonjs schema:sync -d ./src/app.connection.ts",
    "typeorm:cache": "npx typeorm-ts-node-commonjs cache:clear -d ./src/app.connection.ts",
    "schema:drop": "npx typeorm-ts-node-commonjs schema:drop -d ./src/app.connection.ts",
    "migration:create": "npx typeorm-ts-node-commonjs migration:create ./src/migrations/schema-update",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate ./src/migrations/schema-update -d ./src/app.connection.ts",
    "migration:show": "npx typeorm-ts-node-commonjs migration:show -d ./src/app.connection.ts",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d ./src/app.connection.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/app.connection.ts",
    "seeder:run": "ts-node -r tsconfig-paths/register ./src/database/db.seeder.ts",
    "schematic:run": "powershell -ExecutionPolicy Bypass -File .generate.ps1"
  },
  "dependencies": {
    "@fastify/compress": "^7.0.3",
    "@fastify/static": "7.0.1",
    "@fastify/view": "8.2.0",
    "@nestjs/common": "10.3.2",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "10.3.2",
    "@nestjs/platform-fastify": "10.3.2",
    "@nestjs/typeorm": "^10.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "handlebars": "4.7.8",
    "reflect-metadata": "0.2.1",
    "rimraf": "5.0.5",
    "rxjs": "7.8.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.2",
    "@types/express": "4.17.21",
    "@types/node": "20.8.7",
    "@types/supertest": "2.0.16",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.42.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "jest": "29.7.0",
    "prettier": "^3.2.5",
    "supertest": "6.3.3",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  }
}
